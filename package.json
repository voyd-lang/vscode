{
  "name": "mass-lang-vscode",
  "displayName": "mass",
  "description": "VSCode Support For The Mass Programming Language",
  "version": "0.0.4",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:mass-lang/vscode.git"
  },
  "publisher": "mass-lang",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "mass",
        "aliases": [
          "Mass",
          "mass",
          "Mass Lang",
          "Mass Language",
          "Mass Programming Language",
          "mass-lang"
        ],
        "extensions": [
          ".ms",
          ".mass"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mass",
        "scopeName": "index.dm",
        "path": "./syntaxes/mass.tmLanguage.json"
      }
    ]
  }
}
